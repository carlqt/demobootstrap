<% if @books.errors.any? %>
	<div id = "error_explanation">
		<h2>
			<%= pluralize(@books.errors.count, "error")%>
			prohibited this <%= @var %> from being <%= @command %>
		</h2>
		<p>
			<ul>	
				<% @books.errors.full_messages.each do |message|%>
					<li><%= message %></li>
				<% end %>
			</ul>
		</p>
	</div>
<% end %>
